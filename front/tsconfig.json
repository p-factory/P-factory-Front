{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    // 소스 파일의 루트 경로
    // "rootDir": "./src",
    "rootDir": "../",
    // 컴파일 출력 경로
    "outDir": "./dist",

  },
  // front 프로젝트에서 컴파일할 파일 경로
  "include": ["src/**/*","../shared/**/*"],
  // node_modules와 같은 불필요한 파일 제외
  "exclude": ["node_modules"]
}
